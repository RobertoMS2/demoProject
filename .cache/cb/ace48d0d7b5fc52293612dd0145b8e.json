olumn":2311}},{"source":"js/queryUtils.js","name":"outputSeries","original":{"line":96,"column":16},"generated":{"line":1,"column":2314}},{"source":"js/queryUtils.js","name":"element","original":{"line":96,"column":29},"generated":{"line":1,"column":2316}},{"source":"js/queryUtils.js","name":"catch","original":{"line":98,"column":11},"generated":{"line":1,"column":2323}},{"source":"js/queryUtils.js","original":{"line":98,"column":17},"generated":{"line":1,"column":2329}},{"source":"js/queryUtils.js","name":"error","original":{"line":98,"column":17},"generated":{"line":1,"column":2338}},{"source":"js/queryUtils.js","name":"console","original":{"line":99,"column":12},"generated":{"line":1,"column":2341}},{"source":"js/queryUtils.js","name":"error","original":{"line":99,"column":20},"generated":{"line":1,"column":2349}},{"source":"js/queryUtils.js","name":"JSON","original":{"line":99,"column":26},"generated":{"line":1,"column":2355}},{"source":"js/queryUtils.js","name":"stringify","original":{"line":99,"column":31},"generated":{"line":1,"column":2360}},{"source":"js/queryUtils.js","name":"error","original":{"line":99,"column":41},"generated":{"line":1,"column":2370}}],"sources":{"js/queryUtils.js":"const md5 = require('crypto-js/md5');\nconst constants = require('./consts');\n\nconst url_comics = 'https://gateway.marvel.com:443/v1/public/comics?orderBy=-onsaleDate&limit=8&apikey=' + constants.public_key;\nconst url_series = 'https://gateway.marvel.com:443/v1/public/series?orderBy=modified&limit=6&apikey=' + constants.public_key;\nconst url_characters = 'https://gateway.marvel.com:443/v1/public/characters?orderBy=modified&limit=5&apikey=' + constants.public_key;\n\nconst latestContent = document.querySelector('#latestContent');\nconst charactersList = document.querySelector('#charactersList');\nconst latestSeries = document.querySelector('#latestSeries');\n\nfunction outputComics(data) {\n    let item = document.createElement('div');\n    item.setAttribute('class', 'comicItem col col-3');\n    let code = '';\n    code += '<div class=\"r-box r-box-1_1\"><img src=\"' + data.thumbnail.path + '.' + data.thumbnail.extension + '\"></div>';\n    code += '<h3>' + data.title + '</h3>';\n    if (data.description !== null) {\n        code += '<p>' + data.description + '</p>';\n    }\n    code += '<a href=\"./comic.html?id=' + data.id + '\">Ver más <span>&gt&gt</span></a>';\n    item.innerHTML = code;\n    latestContent.appendChild(item);\n}\n\nfunction outputCharacters(data) {\n    let character = document.createElement('div');\n    character.setAttribute('class', 'characterItem');\n    let code = '';\n    code += '<div class=\"r-box r-box-1_2\"><img src=\"' + data.thumbnail.path + '.' + data.thumbnail.extension + '\"></div>';\n    code += '<h3>' + data.name + '</h3>';\n    character.innerHTML = code;\n    charactersList.appendChild(character);\n}\n\nfunction outputSeries(data) {\n    let item = document.createElement('div');\n    item.setAttribute('class', 'comicItem col col-4');\n    let code = '';\n    code += '<div class=\"r-box r-box-1_1\"><img src=\"' + data.thumbnail.path + '.' + data.thumbnail.extension + '\"></div>';\n    code += '<h3>' + data.title + '</h3>';\n    if (data.description !== null) {\n        code += '<p>' + data.description + '</p>';\n    }\n    code += '<a href=\"./series.html?id=' + data.id + '\">Ver más <span>&gt&gt</span></a>';\n    item.innerHTML = code;\n    latestSeries.appendChild(item);\n}\n\nmodule.exports = {\n    getAllcomics: function() {\n        const ts = new Date().getTime();\n        const message = ts+constants.private_key+constants.public_key;\n        const md = md5(message);\n        const hash = md.toString();\n        const url = url_comics + '&ts=' + ts + '&hash=' + hash;\n\n        fetch(url).then(response => {\n            return response.json();\n        }).then(data => {\n            data.data.results.forEach(element => {\n                outputComics(element);\n            });\n        }).catch(error => {\n            console.error(error);\n        });\n    },\n    getCharacters: function() {\n        const ts = new Date().getTime();\n        const message = ts+constants.private_key+constants.public_key;\n        const md = md5(message);\n        const hash = md.toString();\n        const url = url_characters + '&ts=' + ts + '&hash=' + hash;\n        fetch(url).then(response => {\n            return response.json();\n        }).then(data => {\n            charactersList.innerHTML = '';\n            data.data.results.forEach(element => {\n                outputCharacters(element);\n            });\n        }).catch(error => {\n            console.error(JSON.stringify(error));\n        });\n    },\n    getSeries: function() {\n        const ts = new Date().getTime();\n        const message = ts+constants.private_key+constants.public_key;\n        const md = md5(message);\n        const hash = md.toString();\n        const url = url_series + '&ts=' + ts + '&hash=' + hash;\n        fetch(url).then(response => {\n            return response.json();\n        }).then(data => {\n            latestSeries.innerHTML = '';\n            data.data.results.forEach(element => {\n                outputSeries(element);\n            });\n        }).catch(error => {\n            console.error(JSON.stringify(error));\n        });\n    }\n}"},"lineCount":null}},"error":null,"hash":"4889fd92744e9d1abed84d428f283aac","cacheData":{"env":{}}}